{
  "name": "senestro/phpfuser",
  "description": "An almost complete php web fuse",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "beta",
  "prefer-stable": true,
  "version": "12.0",
  "keywords": [
    "phpfuser"
  ],
  "authors": [ {
      "name": "John Habila",
      "email": "Senestro88@gmail.com",
      "role": "Developer"
    } ],
  "support": {
    "email": "Senestro88@gmail.com"
  },
  "autoload": {
    "files": [
      "autoload.php"
    ]
  },
  "lock": false,
  "readme": "README.md",
  "time": "2024-01-10",
  "archive": {
    "name": "PHPFuser"
  },
  "require": {
    "php": ">=8.0",
    "ext-zlib": "*",
    "ext-ftp": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-bz2": "*",
    "ext-fileinfo": "*",
    "james-heinrich/getid3": "^1.9",
    "rakibtg/sleekdb": "^2.15",
    "symfony/process": "^7.2",
    "phpmailer/phpmailer": "^6.9",
    "pear/archive_tar": "^1.5",
    "giggsey/libphonenumber-for-php": "^8.13",
    "guzzlehttp/guzzle": "^7.0",
    "nelexa/zip": "^4.0",
    "phpseclib/phpseclib": "^3.0",
    "symfony/string": "^7.1",
    "symfony/filesystem": "^7.1",
    "vlucas/phpdotenv": "^5.6",
    "maennchen/zipstream-php": "^3.1",
    "nesbot/carbon": "^3.8",
    "chillerlan/php-qrcode": "^5.0",
    "chillerlan/php-authenticator": "^5.2",
    "endroid/qr-code": "^5.1",
    "spatie/image": "^3.7",
    "dompdf/dompdf": "^3.0",
    "mpdf/mpdf": "^8.2",
    "bacon/bacon-qr-code": "^3.0",
    "khanamiryan/qrcode-detector-decoder": "^2.0",
    "cboden/ratchet": "^0.4.4",
    "textalk/websocket": "^1.5",
    "symfony/http-client": "^7.2",
    "gregwar/captcha": "^1.2",
    "php-ffmpeg/php-ffmpeg": "^1.3",
    "matomo/device-detector": "^6.4",
    "symfony/http-foundation": "^6.4",
    "propel/propel": "^2.0@beta"
  },
  "suggest": {
    "ext-libsodium": "SSH2/SFTP can make use of some algorithms provided by the libsodium-php extension.",
    "ext-mcrypt": "Required to speed up a few other cryptographic operations.",
    "ext-openssl": "Required to support encrypt zip entries or use ext-mcrypt",
    "ext-bz2": "Required to support BZIP2 compression",
    "ext-fileinfo": "Required to get mime-type file"
  }
}
